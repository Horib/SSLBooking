@model IQueryable<Route>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @foreach (var p in Model ?? Enumerable.Empty<Route>())
    {
        <div>
            <h3>@p.Name - @p.Price</h3>
            <p>@p.Description</p>
            <p>@p.Location1  => @p.Location2</p>
            <p>@p.Price</p>
        </div>
    }
</div>
